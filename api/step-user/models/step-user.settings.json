{
  "kind": "collectionType",
  "collectionName": "step_users",
  "info": {
    "name": "StepUser"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "step": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "video",
        "quiz",
        "exercise",
        "slide",
        "feedback"
      ],
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "todo",
        "doing",
        "done"
      ],
      "required": true,
      "default": "todo"
    },
    "laststage": {
      "type": "string"
    }
  }
}
