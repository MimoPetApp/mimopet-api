{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "name": "Exercise",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "min_duration": {
      "type": "integer",
      "default": 10
    },
    "max_duration": {
      "type": "integer",
      "default": 20
    },
    "items": {
      "type": "dynamiczone",
      "components": [
        "steps.video-step",
        "steps.feedback-step",
        "steps.slide-step",
        "steps.quiz-step"
      ],
      "required": false
    },
    "pets_completed": {
      "private": true,
      "collection": "pet"
    }
  }
}
