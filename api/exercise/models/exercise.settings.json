{
  "kind": "collectionType",
  "collectionName": "exercises",
  "info": {
    "name": "Exercise",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "min_duration": {
      "type": "integer",
      "default": 10
    },
    "max_duration": {
      "type": "integer",
      "default": 20
    },
    "pets_completed": {
      "collection": "pet"
    },
    "guide": {
      "type": "text"
    },
    "sessions": {
      "type": "dynamiczone",
      "components": [
        "exercises.instruction-exercise",
        "exercises.question-exercise",
        "exercises.animation-exercise"
      ]
    }
  }
}
