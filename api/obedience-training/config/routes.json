{
  "routes": [
    {
      "method": "GET",
      "path": "/obedience-trainings",
      "handler": "obedience-training.find",
      "config": {
        "policies": ["global::is-pet-owner"]
      }
    },
    {
      "method": "GET",
      "path": "/obedience-trainings/count",
      "handler": "obedience-training.count",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/obedience-trainings/:id",
      "handler": "obedience-training.findOne",
      "config": {
        "policies": ["global::is-pet-owner"]
      }
    },
    {
      "method": "POST",
      "path": "/obedience-trainings",
      "handler": "obedience-training.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/obedience-trainings/:id",
      "handler": "obedience-training.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/obedience-trainings/:id",
      "handler": "obedience-training.delete",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/obedience-trainings/:idObedience/exercises/:idExercise/complete/:guideline",
      "handler": "obedience-training.completeGuideline",
      "config": {
        "policies": ["global::is-pet-owner"]
      }
    }
  ]
}
